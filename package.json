{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "husky install",
    "pre:commit": "turbo run pre:commit"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.2",
    "tsconfig": "*",
    "turbo": "^1.10.16"
  },
  "name": "turbo",
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
