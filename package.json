{
  "name": "portfolio-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "env-cmd -f ./.env.development next build",
    "production": "env-cmd -f ./.env.production next build",
    "production:start": "env-cmd -f ./.env.production next start",
    "testing": "env-cmd -f ./.env.testing next build",
    "testing:start": "env-cmd -f ./.env.testing next start",
    "staging": "env-cmd -f ./.env.staging next build",
    "staging:start": "env-cmd -f ./.env.staging next start",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "types": "tsc --noEmit",
    "lint": "npm run types && next lint && eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prepare": "husky install",
    "pre:commit": "lint-staged",
    "postinstall": "husky install",
    "analyze": "ANALYZE=true next build",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --ci --json --coverage --testLocationInResults --outputFile=coverage/report.json",
    "cypress": "cypress open",
    "cypress:ci": "cypress run -b chrome",
    "cypress:ci:firefox": "cypress run -b firefox",
    "cypress:ci:edge": "cypress run -b edge",
    "cypress:ci:webkit": "cypress run -b webkit",
    "e2e": "start-server-and-test testing:start http://localhost:3000 cypress",
    "e2e:ci": "start-server-and-test testing:start http://localhost:3000 cypress:ci:all"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "framer-motion": "^10.16.0",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-resources-to-backend": "^1.1.4",
    "next": "^13.4.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.3.1",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/grecaptcha": "^3.0.4",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "autoprefixer": "10.4.14",
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporters": "^3.1.4",
    "lint-staged": "^13.2.2",
    "next-build-id": "^3.0.0",
    "playwright-webkit": "^1.35.0",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "start-server-and-test": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "**/*.{ts,js,tsx,jsx}": [
      "yarn lint"
    ],
    "**/*.{ts,js,tsx,jsx,json,yml}": [
      "yarn format:write"
    ]
  }
}
