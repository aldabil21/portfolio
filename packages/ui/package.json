{
  "name": "ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "dist/*.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*/index.js",
      "import": "./dist/*/index.mjs",
      "types": "./dist/*/index.d.ts"
    },
    "./styles.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/* --ext .ts,.tsx,.js,.jsx",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "pre:commit": "lint-staged"
  },
  "peerDependencies": {
    "framer-motion": "^10.x.x",
    "react": "^18.2.0",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.48.0",
    "eslint-config-custom": "workspace:*",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.20",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "react": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^5.1.6",
    "utils": "workspace:*"
  },
  "lint-staged": {
    "**/*.{ts,js,tsx,jsx}": [
      "yarn lint"
    ],
    "**/*.{ts,js,tsx,jsx,json,yml}": [
      "yarn format:write"
    ]
  }
}
