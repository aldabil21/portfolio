{
  "name": "utils",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/*/index.ts",
        "src/*.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.ts",
      "default": "./src/index.js",
      "types": "./src/index.ts"
    },
    "./*": {
      "require": "./src/*/index.js",
      "import": "./src/*/index.ts",
      "types": "./src/*/index.ts"
    }
  },
  "scripts": {
    "lint": "eslint src/**/* --ext .ts,.tsx,.js,.jsx"
  },
  "dependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "typescript": "^5.2.2"
  }
}
